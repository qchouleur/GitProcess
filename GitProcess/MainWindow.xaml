                <MenuItem Header="_Import toolbox" Click="OnToolBoxImport"/>
            </MenuItem>
        </Menu>
        <DataGrid x:Name="Processus" Margin="10,30,10,0" VerticalAlignment="Top" Height="238" AutoGenerateColumns="True">
            
        </DataGrid>
    </Grid>
</Window>
